{
  "isDebug": false,
  "logger": {
    "transports": {
      "Loggly": {
        "silent": true,
        "level": "error",
        "handleExceptions": false,
        "json": true,
        "token": "Define_this_key_in_environment_settings",
        "subdomain": "spirehq",
        "tags": ["badge"]
